{
   "manifest_version": 3,
   "name": "代理助手(ProxyAssistant)",
   "version": "1.5.3",
   "description": "一款代理管理插件, 支持 HTTP/HTTPS/SOCKS 代理配置, 支持手动/自动模式切换, 保护隐私, 畅享网络.",
   "browser_specific_settings": {
      "gecko": {
         "id": "proxy-assistant@bugwz.com",
         "strict_min_version": "142.0"
      }
   },
   "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
   },
   "background": {
      "scripts": ["js/worker.js"],
      "type": "module"
   },
   "action": {
      "default_icon": {
         "16": "images/icon-16.png",
         "32": "images/icon-32.png",
         "48": "images/icon-48.png",
         "128": "images/icon-128.png"
      },
      "default_popup": "popup.html",
      "default_title": "代理助手(ProxyAssistant)"
   },
   "host_permissions": [
      "<all_urls>"
   ],
   "permissions": [
      "storage",
      "proxy",
      "webRequest",
      "webRequestAuthProvider"
   ]
}
